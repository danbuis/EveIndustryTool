@page "/blueprint/{BlueprintName}"
@using EveIndustryTool.Models;
@using EveIndustryTool.Blocks
@inject Data.StaticData StaticData

<Blueprint TypeID=BlueprintID />

@code {
    [Parameter]
    public string BlueprintName { get; set; }

    private int BlueprintID;

    protected override void OnInitialized()
    {
        base.OnInitialized();
        BlueprintID = StaticData.MapItemNameToInt(BlueprintName);
    }
}
